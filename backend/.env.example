# Environment
DEBUG=true
ENVIRONMENT=development

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/real_estate_crm

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-min-32-chars
JWT_ISSUER=real-estate-crm
JWT_AUDIENCE=real-estate-crm
JWT_EXPIRES_MIN=480

# AWS S3 Storage (Optional)
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key
AWS_BUCKET_NAME=real-estate-documents
AWS_REGION=us-east-1

# Supabase Storage (Alternative to S3)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key
SUPABASE_BUCKET=documents

# CORS & Security
CORS_ORIGINS=["http://localhost:5173","http://localhost:3000","https://your-frontend.com"]
ALLOWED_HOSTS=["*"]

# Rate Limiting
RATE_LIMIT_LOGIN=5
RATE_LIMIT_MUTATION=30